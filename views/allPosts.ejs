  <div class="row" style="margin-top: 90px;">
    <div class="col-12">
      <% projects.forEach(element => { %>
      <div class="card">
        <div class="card-body">
          <h5 class="card-title name">Name: <%= element.userId.fullName %> </h5>
          <!-- <h6 class="card-subtitle mb-2 text-muted">Organisation Name: TCET</h6> -->
          <p class="card-text phoneNo">Contact No: <%= element.userId.phoneNo %> </p>
          <p class="card-text"><i class="fas fa-building"></i> <%= element.userId.organisationName %> </p>
          <p class="card-text">Project Name: <%= element.projectName %> </p>
          <p class="card-text">Project Description: <%= element.projectDescription %> </p>
        </div>
        <div class="card-footer">
          <a href="mailto:<%= element.userId.username %> " role="button"
            class="btn btn-primary btn-md float-right">Contact</a>
        </div>
      </div>
      <% }) %>
    </div>
  </div>



  <script src="https://code.jquery.com/jquery-3.5.1.slim.min.js"
