<!DOCTYPE html>
<html lang="en">

<head>
  <meta charset="UTF-8">
  <meta name="viewport" content="width=device-width, initial-scale=1.0">
  <title>Startup India</title>
  <!-- Css Links -->
  <link rel="stylesheet" href="https://cdn.jsdelivr.net/npm/bootstrap@4.5.3/dist/css/bootstrap.min.css"
    integrity="sha384-TX8t27EcRE3e/ihU7zmQxVncDAy5uIKz4rEkgIXeMed4M0jlfIDPvg6uqKI2xXr2" crossorigin="anonymous">
  <link rel="stylesheet" type="text/css" href="../css/styles.css">
  <script src="https://kit.fontawesome.com/f33983bb23.js" crossorigin="anonymous"></script>
  <link rel="stylesheet" type="text/css" href="../css/bootstrap-social.css">
  <link rel="icon" href="../images/favicon-32x32.png">
</head>

<body>
  <%- include ("partials/navbar.ejs"); -%>
  <div class="fluid-container" style="margin-top: 100px;">
    <table class="table table-striped">
      <thead>
        <tr>
          <th>Project Name</th>
          <th>Investor Name</th>
          <th>Investor Proposal</th>
          <th>Bid Amount</th>
          <th>Approve</th>
        </tr>
      </thead>
      <tbody>
        <% foundAllInvestors.forEach(element => { %>
        <% element.investAmount.forEach(item => { %>
        <tr>
          <td><%= element.projectName %> </td>
          <td><%= item.investorId.fullName %> </td>
          <td><%= item.proposal %> </td>
          <td><%= item.amount %> </td>
          <td><a href="" role="button" class="btn btn-md btn-success">Approve</a></td>
        </tr>
        <% }); %>

        <% }); %>
      </tbody>
    </table>
  </div>
</body>

</html>